{
  "family": "survey_tool-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "survey_tool-container",
      "image": "your-image:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "memory": 512,
      "cpu": 256
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
