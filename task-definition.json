{
  "family": "survey_tool-task",
  "executionRoleArn": "arn:aws:iam::897067486570:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "survey_tool-container",
      "image": "YOUR_ECR_REPO_URI:TAG",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
